{
  "name": "image-video-generation-mcp",
  "version": "1.0.2",
  "description": "MCP server for image and video generation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "image-video-generation-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "debug": "node --inspect --inspect-port=9229 dist/index.js",
    "debug:break": "node --inspect-brk --inspect-port=9229 dist/index.js",
    "inspector": "node scripts/inspector.js",
    "inspector:direct": "npx @modelcontextprotocol/inspector node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "pnpm build && pnpm test",
    "prepack": "pnpm build",
    "release": "pnpm build && pnpm test && npm publish"
  },
  "keywords": [
    "mcp",
    "image-generation",
    "video-generation",
    "ai",
    "cogview",
    "cogvideox"
  ],
  "author": "156554395 <156554395@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/156554395/image-video-generation-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/156554395/image-video-generation-mcp/issues"
  },
  "homepage": "https://github.com/156554395/image-video-generation-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@modelcontextprotocol/inspector": "^0.17.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
